{
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "System": "Trace",
            "Microsoft": "Trace",
            "ElkCreekServices.OpenScripts.Logging.Example": "Trace"
        },
        "RotatingFile": {
            "Configurations": [
                {
                    "LogLevel": "None",
                    "ConsoleLoggingEnabled": true,
                    "ConsoleMinLevel": "Error",
                    "Filename": "./logging/log_default.txt",
                    "IncludeDateTime": true,
                    "IsUtcTime": true,
                    "PurgeAfterDays": 2,
                    "AutoGenerateDirectory": true,
                    "AttemptAutoFileRenameOnIOException": true //,
                    //"MaximumLogFileSizeKB": "5"
                },
                {
                    "Name": "custom",
                    "LogLevel": "None",
                    "ConsoleLoggingEnabled": true,
                    "ConsoleMinLevel": "Trace",
                    "Filename": "./logging/log_custom.txt",
                    "IncludeDateTime": true,
                    "IsUtcTime": true,
                    "PurgeAfterDays": 4,
                    "AutoGenerateDirectory": true,
                    "AttemptAutoFileRenameOnIOException": true,
                    "MaximumLogFileSizeKB": "5"
                },
                {
                    "Name": "chat",
                    "LogLevel": "None",
                    "ConsoleLoggingEnabled": true,
                    "ConsoleMinLevel": "Trace",
                    "Filename": "./logging/log_chat.txt",
                    "IncludeDateTime": true,
                    "IsUtcTime": true,
                    "PurgeAfterDays": 2,
                    "AutoGenerateDirectory": true,
                    "AttemptAutoFileRenameOnIOException": true,
                    "MaximumLogFileSizeKB": "5"
                }
            ]
        }
    }
   
}
